<div class="container">
  <div class="new_form">
    <%= simple_form_for(@offering) do |f| %>
      <%= f.input :title%>
      <%= f.input :available %>
      <%= f.input :description %>
      <%= f.input :type %>
      <%= f.input :price_per_night %>
      <%= f.input :address %>
      <%= f.input :city %>
      <%= f.input :country, as: :text %>
       <%= f.input :size %>
      <%= f.submit %>
    <% end %>
  </div>
</div>
